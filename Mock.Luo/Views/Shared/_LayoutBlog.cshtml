@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewBag.Title</title>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="renderer" content="webkit|ie-comp|ie-stand">
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1 user-scalable=no">
    <meta name="description" content="分享知识，分享快乐，分享生活。" />
    <meta name="keywords" content="如莫博客,个人博客,独立博客,博客,程序员,奋斗的扫雪工" />
    <link href="https://cdn.bootcss.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <link href="https://cdn.bootcss.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/js/layui/css/layui.css" rel="stylesheet" />
    <link href="~/Content/css/global.blog.css" rel="stylesheet" />
    @RenderSection("css", required: false)
    <!--[if lt IE 9]>
    <script src="https://cdn.bootcss.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://cdn.bootcss.com/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <script src="https://cdn.bootcss.com/jquery/2.1.4/jquery.min.js"></script>
    <style>
    </style>
</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top clearfix">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <span class="navbar-brand">Mock Blog</span>
            </div>

            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                    <li><a href="#"><span class="glyphicon glyphicon-home"></span> 本站首页 <span class="sr-only">(current)</span></a></li>
                    <li><a href="#"><span class="glyphicon glyphicon-book"></span> 博文</a></li>
                    <li><a href="#"><span class="glyphicon glyphicon-header"></span> 随笔</a></li>
                    <li><a href="#"><span class="glyphicon glyphicon-stats"></span> 关于我</a></li>
                    <li><a href="#"><span class="glyphicon glyphicon-compressed"></span> 留言</a></li>
                </ul>
            </div>
        </div>
    </nav>
    @RenderBody()

    <footer class="footer">
        <div class="container">
            <div class="col-xs-12 col-sm-12 col-lg-12">
                <div class="col-xs-12 col-sm-6">
                    <div class="col-xs-12 col-sm-12">
                        <p><i class="fa fa-link"></i><a href="/" target="_blank"> 友情链接</a></p>
                    </div>
                    <div class="col-xs-12 col-sm-12">
                        <ul class="footer_links">
                            <li><a href="/" target="_blank"></a></li>
                            <li><a href="/" target="_blank"></a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-3">
                    <div class="col-xs-12 col-sm-12">
                        <p><i class="fa fa-desktop"></i> 关于本站</p>
                    </div>
                    <div class="col-xs-12 col-sm-12">
                        <p>
                            本站是如莫的个人博客，专注个人学习，成长，留言，记录。欢迎沟通交流！
                        </p>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-3">
                    <div class="col-xs-12 col-sm-12 ">
                        <p><i class="fa fa-weixin"></i> 微信公众号</p>
                    </div>
                    <div class="col-xs-12 col-sm-12">
                        <p><img src="https://www.gzpblog.com/wp-content/themes/SaltedFish/public/images/wechatPic.png" alt="微信公众号二维码" style="width:150px;"></p>
                    </div>
                </div>
            </div>
        </div>
        <div class="container-fluid">
            <div class="col-xs-12 col-sm-12 col-lg-12  text-center">
                <span>
                    Copyright © 2017 <a id="footer_name" href="https://www.gzpblog.com" target="_blank">如莫博客</a>
                    All Rights Reserved. <a href="https://www.gzpblog.com/20170601/986.html" target="_blank" rel="nofollow">SaltedFish</a> theme powered by <a href="https://www.gzpblog.com" target="_blank" rel="nofollow">guo</a>.
                    &nbsp;|&nbsp;
                    <a href="http://www.miitbeian.gov.cn/" target="_blank" rel="nofollow">粤ICP备16013442号</a>
                    &nbsp;|&nbsp;
                    <a href="/sitemap.xml" target="_blank">网站地图</a>
                    &nbsp;|&nbsp;
                    本站运行于 <img src="https://www.gzpblog.com/wp-content/themes/SaltedFish/public/images/aliyun.png" style="height:30px;">
                    &nbsp;|&nbsp;
                </span>
            </div>
        </div>
    </footer>

    <ul id="scroll">
        <li class="log log-no"><a class="log-button" title="文章目录"><i class="fa fa-bars"></i></a><div class="log-prompt" style="display: none;"><div class="log-arrow">文章目录</div></div></li>
        <li><a class="scroll-h" title="返回顶部"><i class="fa fa-angle-up"></i></a></li>
        <li><a class="scroll-c" title="评论"><i class="fa fa-comment-o"></i></a></li>
        <li><a class="scroll-b" title="转到底部"><i class="fa fa-angle-down"></i></a></li>
        <li class="qr-site"><a href="javascript:void(0)" class="qr" title="本页二维码"><i class="fa fa-qrcode"></i><span class="qr-img"><span id="output"><img class="alignnone" src="http://www.99bsy.com/wp-content/uploads/2017/07/81sy.png" alt="icon"></span><span class="arrow arrow-z"><i class="fa fa-caret-right"></i></span><span class="arrow arrow-y"><i class="fa fa-caret-right"></i></span></span></a></li>
    </ul>


    <script src="https://cdn.bootcss.com/bootstrap/3.3.6/js/bootstrap.js"></script>
    <script src="https://cdn.bootcss.com/vue/2.4.2/vue.min.js"></script>
    @*<script src="https://cdn.bootcss.com/layer/3.0.3/layer.min.js"></script>*@
    <script src="~/Content/js/layui/layui.js"></script>
    <script src="https://cdn.bootcss.com/jquery.qrcode/1.0/jquery.qrcode.min.js"></script>
    <script src="~/Content/js/global.js"></script>
    <script>
        $(function () {
            $('.scroll-h').click(function () {
                $('html,body').animate({
                    scrollTop: '0px'
                },
                800);
            });
            $('.scroll-c').click(function () {
                $('html,body').animate({
                    scrollTop: $('.scroll-comments').offset().top
                },
                800);
            });
            $('.scroll-b').click(function () {
                $('html,body').animate({
                    scrollTop: $('.footer').offset().top
                },
                800);
            });
            if (!+[1, ]) { present = "table"; } else { present = "canvas"; } $('#output').qrcode({ render: present, text: window.location.href, width: "150", height: "150" });


            $(".qr").mouseover(function () {
                $(this).children(".qr-img").show();
            });
            $(".qr").mouseout(function () {
                $(this).children(".qr-img").hide();
            });
        });
    </script>

    @RenderSection("scripts", required: false)
</body>
</html>
